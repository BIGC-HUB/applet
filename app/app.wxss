/* 图标库 */
@import './packages/iconfont.wxss';

div,
header,
main,
footer,
page,
view,
scroll-view,
swiper,
movable-area,
cover-view,
cover-image,
text,
icon,
rich-text,
progress,
button,
checkbox-group,
checkbox,
form,
input,
label,
picker,
picker-view,
radio-group,
slider,
switch,
textarea,
navigator,
audio,
image,
video,
map,
live-player,
live-pusher,
open-data,
web-view {
	box-sizing: border-box;
}

div {
	display: block;
}

image {
	width: 100%;
	height: 100%;
}

button {
	background: transparent;
	outline: none;
	padding: 0;
	border: 0;
	margin: 0;
	line-height: 1;
}
button::after {
	border: none;
}
button.button-hover {
	background: transparent;
}

input {
	outline: none;
	border: none;
	list-style: none;
}
